def media(p1,p2):
  m = (p1 + p2) / 2
def verificar(m):
  if m >= 6:
    return "Aprovado!!!!!!"
  else:
    return "Reprovado!!!!!"
def main():
  n1 = float(input("Digite a nota da P1: "))
  n2 = float(input("Digite a nota da P2: "))
  m = (n1 + n2) / 2
  media(n1,n2)
  verificar(m)
  print("O aluno foi: ", verificar(m))
main()
